{
  "name": "supply-chain-server",
  "version": "1.0.0",
  "description": "rest api server",
  "main": "app.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/app.js",
    "dev": "PORT=4000 concurrently \"npx tsc --watch\" \"nodemon -q dist/app.js\"",
    "prisma:create": "npx prisma migrate dev --create-only",
    "prisma:mig": "npx prisma migrate dev --name init",
    "prisma:db": "npx prisma db ",
    "prisma:format": "npx prisma format ",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio ",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:dump": "npx prisma migrate reset --force && npx prisma migrate deploy"
  },
  "author": "victormutaijambo@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "random-word-slugs": "^0.1.7",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.3",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
